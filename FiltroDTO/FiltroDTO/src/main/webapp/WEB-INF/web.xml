<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <filter>
        <filter-name>NameWebFilter</filter-name>
        <filter-class>com.arelance.filtrodto.NameWebFilter</filter-class>
    </filter>
    <filter>
        <filter-name>MinWebFilter</filter-name>
        <filter-class>com.arelance.filtrodto.MinWebFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>NameWebFilter</filter-name>
        <url-pattern>/Destino</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>
    <filter-mapping>
        <filter-name>MinWebFilter</filter-name>
        <url-pattern>/Destino</url-pattern>
        <dispatcher>REQUEST</dispatcher>
        <dispatcher>FORWARD</dispatcher>
    </filter-mapping>
    <listener>
        <description>ServletContextListener</description>
        <listener-class>com.arelance.filtrodto.listener.NewServletListener</listener-class>
    </listener>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
